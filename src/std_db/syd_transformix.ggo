#File syd_transformix.ggo

package "syd"
version "0.4"
usage "syd_transformix <transform> <image> "
description "Parameters are:
- <image>         : ids of the image to transform
"

option "transform"   	        -       "List of transform ids (between \"\")"        string    yes
option "tags"   	        t       "Add tags (separated by space, in \"\")"      string    default="transform" no