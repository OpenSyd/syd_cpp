        </references>
      </foreign-key>
      <index name="syd::DicomSerie_spacing_object_id_i">
        <column name="object_id"/>
      </index>
      <index name="syd::DicomSerie_spacing_index_i">
        <column name="index"/>
      </index>
    </add-table>
