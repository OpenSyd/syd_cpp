#File sydCommandLineWithFitOptions.ggo

option "r2_min" - "Min R2 threshold for model selection" float default="0.9" no
option "iterations" i "Max nb of iterations" int default="50" no
option "restricted_tac"  r "Fit only end of the curve (max to end)" flag off
option "model" m "Set the model list use to fit f2 f3 f4a etc" string multiple no
option "akaike" a "Set the Akaike criterion AIC or AICc" string default="AIC" no
